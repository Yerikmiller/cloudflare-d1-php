{
    "name": "kolosos/cloudflare-d1-php",
    "description": "A lightweight PHP client for Cloudflare D1 database",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yorman",
            "email": "yorman@kolososdev.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Cloudflare\\D1\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cloudflare\\D1\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
